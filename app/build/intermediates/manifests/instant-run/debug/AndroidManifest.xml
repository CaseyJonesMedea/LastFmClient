<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="by.viachaslau.kukhto.lastfmclient"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        name="by.viachaslau.kukhto.lastfmclient.Others.App"
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/lastfm_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="by.viachaslau.kukhto.lastfmclient.View.WelcomeActivity.WelcomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="by.viachaslau.kukhto.lastfmclient.View.UserActivity.UserActivity" >
        </activity>
        <activity android:name="by.viachaslau.kukhto.lastfmclient.View.ArtistActivity.ArtistActivity" />
        <activity android:name="by.viachaslau.kukhto.lastfmclient.View.AlbumActivity.AlbumActivity" />
        <activity android:name="by.viachaslau.kukhto.lastfmclient.View.ListActivity.ListActivity" />
        <activity
            android:name="com.karumi.dexter.DexterActivity"
            android:launchMode="singleTask"
            android:theme="@style/Theme.Transparent" />
    </application>

</manifest>